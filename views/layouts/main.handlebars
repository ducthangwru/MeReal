<body class="hold-transition skin-blue sidebar-mini">
    <header class="main-header">
        <a href="/" class="logo">
            <span class="logo-mini"><b>ME</b>R</span>
            <span class="logo-lg"><b>ME</b>REAL</span>
        </a>

        <nav class="navbar navbar-static-top">
            <a href="#" class="sidebar-toggle" data-toggle="push-menu" role="button">
                <span class="sr-only">Toggle navigation</span>
            </a>
            <div class="navbar-custom-menu">
                <ul class="nav navbar-nav">
                    <!-- User Account: style can be found in dropdown.less -->
                    <li class="dropdown user user-menu">
                        <a href="#" class="dropdown-toggle" style="display: -webkit-inline-box;" data-toggle="dropdown">
                            <span id="spanFullname" class="hidden-xs"></span>
                        </a>
                        <ul class="dropdown-menu">
                            <!-- User image -->
                            <li class="user-header">
                                <div id="divFullname" class="textavatar" data-name="{{user.fullname}}"></div>
                                <p id="pUsername">
                                </p>
                            </li>
                            <!-- Menu Body -->
                            <li class="user-body">
                                <div class="row">
                                    <div class="col-xs-12 text-center">
                                        <a href="#">Lịch sử chơi</a>
                                    </div>
                                </div>
                                <!-- /.row -->
                            </li>
                            <!-- Menu Footer-->
                            <li class="user-footer">
                                <div class="pull-left">
                                    <a href="/profile" class="btn btn-default btn-flat">TT Tài khoản</a>
                                </div>
                                <div class="pull-right">
                                    <a href="/logout" class="btn btn-default btn-flat">Đăng xuất</a>
                                </div>
                            </li>
                        </ul>
                    </li>
                </ul>
            </div>
        </nav>
    </header>
    <aside class="main-sidebar">
        <section class="sidebar">
            <ul id="ulMenu" class="sidebar-menu" data-widget="tree">
                <li><a href="/"><i class="fa fa-dashboard"></i> <span>Trang chủ</span></a></li>
            </ul>
        </section>
        <!-- /.sidebar -->
    </aside>

    <script src="js/jquery.js"></script>
    <script src="js/jquery.textavatar.js"></script>
